{
	"name": "Ponto Flexível",
	"description": "Aplicativo para calcular a hora da saída do trabalho para jornadas de trabalho flexíveis.",
    "version": "0.0.1",
    "permissions": [
    	"tabs", "contextMenus"
  	],
	"icons": { "16": "images/icon.png",
	 		   "48": "images/48.png",
	 		   "128": "images/128.png"},
	"browser_action": {
		"default_title": "Ponto Flexível",
		"default_icon": "images/icon.png",
		"default_popup": "popup.html"
	},
	"manifest_version": 2
}